{
    "name": "nagi1/omnipay-myfatoorah",
    "description": "MyFatoorah driver for the Omnipay PHP payment processing library",
    "keywords": [
        "omnipay",
        "myfatoorah"
    ],
    "homepage": "https://github.com/nagi1/omnipay-myfatoorah",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Nagi",
            "email": "info@ahmednagi.com",
            "homepage": "https://ahmednagi.com"
        }
    ],
    "require": {
        "omnipay/common": "^3"
    },
    "require-dev": {
        "omnipay/tests": "^3",
        "squizlabs/php_codesniffer": "^3",
        "phpro/grumphp": "^0.14"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\MyFatoorah\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\MyFatoorah\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 src/",
        "fix-style": "phpcbf -p --standard=PSR2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
